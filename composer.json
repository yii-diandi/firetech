{
    "name": "yii-diandi/firetech",
    "description": "店滴AI-基于AI技术的应用开源管理系统，对接AI有关的软件、硬件，提供基于AI技术的整体解决方案",
    "keywords": [
        "yii2",
        "ai开源框架",
        "python开源框架"
    ],
    "homepage": "http://www.wayfirer.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://gitee.com/wayfiretech_admin/firetech/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "https://gitee.com/wayfiretech_admin/firetech/wikis",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://gitee.com/wayfiretech_admin/firetech"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2.0",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "yiisoft/yii2-redis": "^2.0",
        "yiisoft/yii2-queue": "^2.3",
        "yiisoft/yii2-jui": "^2.0",
        "yiisoft/yii2-bootstrap": "^2.0",
        "yii2mod/yii2-enum": "~1.7.1",
        "yii2mod/yii2-swagger": "~1.1.1",
        "yii2mod/yii2-settings": "~2.5",
        "overtrue/wechat": "~4.0",
        "leandrogehlen/yii2-treegrid": "~1.1.0",
        "richardfan1126/yii2-js-register": "~0.0.1",
        "ramsey/uuid": "^3.9",
        "overtrue/easy-sms": "^1.1",
        "yii-diandi/yii2-admin": "^2.24",
        "yii-diandi/yii2-region": "^1.0",
        "yii-diandi/yii2-migration": "^2.0",
        "yii-diandi/yii2-swoole": "^1.98",
        "yii-diandi/yii2-addons": "^1.9",
        "phpoffice/phpspreadsheet": "^1.14",
        "yiisoft/yii2-smarty": "^2.0",
        "2amigos/qrcode-library": "^1.1",
        "hashids/hashids": "^4.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "phpunit/phpunit": "~6.5.5",
        "symfony/browser-kit": "4.2.4",
        "yiisoft/yii2-gii": "^2.1",
        "codeception/verify": "~1.0.0",
        "codeception/base": "^2.4.0"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
